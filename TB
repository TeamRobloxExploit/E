section1:addToggle("Auto Farm", nil, function(value)
print("Toggled", value)
end)
  
section2:addButton("Inf money + candy", function()
game.StarterGui:SetCore("SendNotification", {
Title = "Inf money"; -- the title
Text = "Started"; -- what the text says
Duration = 3; -- how long the notification should in secounds
})
_G.autospin = true
_G.speed = 200 -- this is how fast it should go--
if _G.autospin == true then
for i=1,_G.speed do
    print(i)
spawn(function()
while _G.autospin do
game:GetService("ReplicatedStorage").SpinWheel:InvokeServer()
wait(1)
end
end)
end
end
end)
-- second page
local page = venyx:addPage("Player", 5012544693)
local credit = page:addSection("Coming soon")


--thrid page
local theme = venyx:addPage("Gui", 5012544693)
local colors = theme:addSection("Colors")
local gui = theme:addSection("Gui")

for theme, color in pairs(themes) do -- all in one theme changer, i know, im cool
colors:addColorPicker(theme, color, function(color3)
venyx:setTheme(theme, color3)
end)
end


gui:addButton("Destroy Gui", function()
for i,v in pairs(game.CoreGui:GetDescendants()) do
if v.Name == "Ultimate Hub | Car Dealership Tycoon" then
v:Destroy()
end
end
end)

